{
  "manifest_version": 3,
  "name": "Gmail Filter: Show Only New Mail",
  "version": "0.1.0",
  "description": "Show only new Gmail messages since your last session.",
  "key": "",
  "action": {
    "default_title": "New Mail",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": ["storage", "identity", "alarms"],
  "host_permissions": ["https://mail.google.com/*", "https://www.googleapis.com/*"],
  "oauth2": {
    "client_id": "1070443905646-39l974b88lkfjpa314rbl0tpk717pa5v.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/gmail.readonly"]
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content_script.js"],
      "css": ["content_script.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
